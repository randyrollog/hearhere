<div class="page_container">
	<% if @sounds.empty? %>
	  <h2>No sounds found!</h2>
	  <div id="youshould">
	  	You should upload it! <br>
	  	<%= link_to "Login", new_authentication_path %> or <%= link_to "Sign Up", new_user_path %> to upload
	  </div>

	<% else %>
	  <%= render "sounds/recent" %>
	<% end %>
</div>