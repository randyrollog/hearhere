<div class="page_container">
  <div class="key_icon"><%= image_tag "keys.svg", class: "key_icon" %></div>
  <h1>Login</h1>
    <div id="super-container">
    <%= form_for @user, {url:"/authentications", method:"post"} do |u| %>
      <p class="auth_form"><%= u.text_field :user_name, placeholder: 'Username', class: "auth_form" %></p>
      <p class="auth_form"><%= u.password_field :password, placeholder: 'Password', class: "auth_form" %></p>
      <p class="auth_form"><%= u.submit "Log In", class: "auth_btn" %></p>
    <% end %>
    </div>
</div>